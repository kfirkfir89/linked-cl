{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "target": "ESNext",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
  },
  "include": [
    "./.eslintrc.cjs",
    "src/index.d.ts",
    "./*",
    "./*.js",
    "./*.ts",
    "src/**/*.js",
    "src/**/*.ts",
    "test/**/*.ts", "src/services/adobe-api/create-pdf-from-json/index.js", 
  ],

}